{
	"manifest_version": 2,

	"name" : "SLTrib+",
	"description" : "Make The Trib Great Again",
	"version" : "0.3",

"permissions": [
	"tabs",
    "http://www.sltrib.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.sltrib.com/*"],
      "js": ["popup.js"],
      "css": ["hider.css"],
      "run_at": "document_end"
    }],
 "page_action": 
 {
	"default_icon": "favicon.png"
 }

}

